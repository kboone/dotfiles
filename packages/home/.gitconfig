[user]
    email = kboone@berkeley.edu
    name = Kyle Boone

[alias]
    co = checkout
    ci = commit
    st = status
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    type = cat-file -t
    dump = cat-file -p

[push]
    default = current
[core]
	editor = nvim
[credential]
	helper = cache
